CREATE TRIGGER TR_ALF_SENHA
BEFORE INSERT ON funcionario
FOR EACH ROW
BEGIN
	IF (NEW.SENHA NOT LIKE '%[0-9]%' AND NEW.SENHA NOT LIKE '%[a-zA-Z]%') THEN
		SET NEW.SENHA = 1/0;
	END IF;
END;;


ALTER TABLE funcionario
ADD CONSTRAINT ALF_SENHA
CHECK ((SENHA LIKE '%[0-9]%') AND (SENHA LIKE '%[a-zA-Z]%'));
